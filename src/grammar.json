{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "ink",
  "rules": {
    "program": {
      "type": "PREC",
      "value": 1,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "ALIAS",
                "content": {
                  "type": "SYMBOL",
                  "name": "empty_line"
                },
                "named": false,
                "value": ""
              },
              {
                "type": "BLANK"
              }
            ]
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "weave_body"
              },
              {
                "type": "BLANK"
              }
            ]
          },
          {
            "type": "REPEAT",
            "content": {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "knot"
                },
                {
                  "type": "SYMBOL",
                  "name": "function"
                }
              ]
            }
          }
        ]
      }
    },
    "knot": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "knot_header"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "weave_body"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "stitch"
          }
        }
      ]
    },
    "knot_header": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "knot_start"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "PATTERN",
              "value": "=+"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "PATTERN",
              "value": "=+"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "line_end"
        }
      ]
    },
    "stitch": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "stitch_header"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "weave_body"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "stitch_header": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "stitch_start"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "SYMBOL",
          "name": "line_end"
        }
      ]
    },
    "weave_body": {
      "type": "PREC_RIGHT",
      "value": 0,
      "content": {
        "type": "REPEAT1",
        "content": {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "weave_body_line"
            },
            {
              "type": "ALIAS",
              "content": {
                "type": "SYMBOL",
                "name": "empty_line"
              },
              "named": false,
              "value": ""
            }
          ]
        }
      }
    },
    "weave_body_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "body_start"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "choice_text"
                },
                {
                  "type": "SYMBOL",
                  "name": "code_text"
                },
                {
                  "type": "SYMBOL",
                  "name": "dialog_text"
                },
                {
                  "type": "SYMBOL",
                  "name": "gather_text"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "line_end"
        }
      ]
    },
    "function": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "function_header"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "function_body"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "gather_text": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "gather_mark"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "dialog_text"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "gather_mark": {
      "type": "REPEAT1",
      "content": {
        "type": "PATTERN",
        "value": "-"
      }
    },
    "choice_text": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "choice_mark"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "dialog_text"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "choice_mark": {
      "type": "REPEAT1",
      "content": {
        "type": "PATTERN",
        "value": "[\\+\\*]"
      }
    },
    "code_text": {
      "type": "SEQ",
      "members": [
        {
          "type": "PATTERN",
          "value": "~"
        },
        {
          "type": "SYMBOL",
          "name": "text"
        }
      ]
    },
    "dialog_text": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "text"
        },
        {
          "type": "SYMBOL",
          "name": "divert"
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "text"
            },
            {
              "type": "SYMBOL",
              "name": "divert"
            }
          ]
        }
      ]
    },
    "text": {
      "type": "REPEAT1",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "vocabular"
          },
          {
            "type": "SYMBOL",
            "name": "other"
          }
        ]
      }
    },
    "divert": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "arrow"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        }
      ]
    },
    "function_header": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "function_start"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "PATTERN",
              "value": "=+"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "PATTERN",
              "value": "\\("
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "arguments"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "PATTERN",
              "value": "\\)"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "PATTERN",
              "value": "=+"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "line_end"
        }
      ]
    },
    "function_body": {
      "type": "PREC_RIGHT",
      "value": 0,
      "content": {
        "type": "REPEAT1",
        "content": {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "function_body_line"
            },
            {
              "type": "ALIAS",
              "content": {
                "type": "SYMBOL",
                "name": "empty_line"
              },
              "named": false,
              "value": ""
            }
          ]
        }
      }
    },
    "function_body_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "body_start"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "code_text"
            },
            {
              "type": "SYMBOL",
              "name": "dialog_text"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "line_end"
        }
      ]
    },
    "arguments": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "PATTERN",
                "value": ","
              },
              {
                "type": "SYMBOL",
                "name": "identifier"
              }
            ]
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "PATTERN",
              "value": ","
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "other": {
      "type": "PATTERN",
      "value": "[^\\s\\n\\r\\p{L}_]+"
    },
    "vocabular": {
      "type": "PATTERN",
      "value": "[\\p{L}_-]+"
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[\\p{L}_]+"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "[ \\t\\v\\f\\u00A0\\u1680\\u2000\\u2001\\u2002\\u2003\\u2004\\u2005\\u2006\\u2007\\u2008\\u2009\\u200A\\u2028\\u2029\\u202F\\u205F\\u3000]"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [
    {
      "type": "SYMBOL",
      "name": "arrow"
    },
    {
      "type": "SYMBOL",
      "name": "body_start"
    },
    {
      "type": "SYMBOL",
      "name": "stitch_start"
    },
    {
      "type": "SYMBOL",
      "name": "knot_start"
    },
    {
      "type": "SYMBOL",
      "name": "function_start"
    },
    {
      "type": "SYMBOL",
      "name": "empty_line"
    },
    {
      "type": "SYMBOL",
      "name": "line_end"
    }
  ],
  "inline": [],
  "supertypes": []
}
