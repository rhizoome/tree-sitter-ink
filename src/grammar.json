{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "ink",
  "rules": {
    "program": {
      "type": "SEQ",
      "members": [
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "body"
          }
        }
      ]
    },
    "body": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "line_start"
        },
        {
          "type": "SYMBOL",
          "name": "body_line"
        },
        {
          "type": "PATTERN",
          "value": "\\n"
        }
      ]
    },
    "body_line": {
      "type": "REPEAT1",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "arrow"
          },
          {
            "type": "SYMBOL",
            "name": "vocabular"
          }
        ]
      }
    },
    "vocabular": {
      "type": "PREC_RIGHT",
      "value": 0,
      "content": {
        "type": "REPEAT1",
        "content": {
          "type": "CHOICE",
          "members": [
            {
              "type": "PATTERN",
              "value": "[\\p{L}_]+"
            },
            {
              "type": "ALIAS",
              "content": {
                "type": "SYMBOL",
                "name": "minus"
              },
              "named": false,
              "value": "-"
            }
          ]
        }
      }
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [
    {
      "type": "SYMBOL",
      "name": "arrow"
    },
    {
      "type": "SYMBOL",
      "name": "minus"
    },
    {
      "type": "SYMBOL",
      "name": "line_start"
    }
  ],
  "inline": [],
  "supertypes": []
}
