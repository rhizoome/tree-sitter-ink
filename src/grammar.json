{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "ink",
  "rules": {
    "program": {
      "type": "REPEAT",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "body_start"
              },
              {
                "type": "SYMBOL",
                "name": "knot_start"
              },
              {
                "type": "SYMBOL",
                "name": "choice_start"
              }
            ]
          },
          {
            "type": "REPEAT",
            "content": {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "arrow"
                },
                {
                  "type": "SYMBOL",
                  "name": "minus"
                },
                {
                  "type": "SYMBOL",
                  "name": "other"
                }
              ]
            }
          },
          {
            "type": "SYMBOL",
            "name": "line_end"
          }
        ]
      }
    },
    "other": {
      "type": "PREC",
      "value": -1,
      "content": {
        "type": "PATTERN",
        "value": "."
      }
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [
    {
      "type": "SYMBOL",
      "name": "arrow"
    },
    {
      "type": "SYMBOL",
      "name": "minus"
    },
    {
      "type": "SYMBOL",
      "name": "body_start"
    },
    {
      "type": "SYMBOL",
      "name": "knot_start"
    },
    {
      "type": "SYMBOL",
      "name": "choice_start"
    },
    {
      "type": "SYMBOL",
      "name": "line_end"
    }
  ],
  "inline": [],
  "supertypes": []
}
